    {
        EsFurioso = true;
    }

    public Gato(bool esFurioso) // Polimorfismo
    {
        EsFurioso = esFurioso;
    }
    public void Rugir()
    {
        Console.WriteLine("animal dominante y posesivo con sus due√±os. depredador que se alimenta de carne");

    }

}